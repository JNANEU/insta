<!DOCTYPE html>
<html>
  <head>
    <title>Instagram</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
  <span class="navbar-brand"><%= link_to 'University', univesity_contacts_path %></span>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item active">
        <span class="nav-link"><%= link_to "Group", groups_path %><span class="sr-only">(current)</span></span>
      </li>
      <li class="nav-item">
        <span class="nav-link"><%= link_to "Students", students_path %></span>
      </li>
      <li class="nav-item">
        <span class="nav-link"><%= link_to "Faculty", faculties_path %></span>
      </li>
      <li class="nav-item">
        <span class="nav-link disabled"tabindex="-1" aria-disabled="true">Personal cabinet</span>
      </li>
    </ul>
  </div>
</nav>
    
    
    
    
    <%= yield %>
  </body>
</html>
